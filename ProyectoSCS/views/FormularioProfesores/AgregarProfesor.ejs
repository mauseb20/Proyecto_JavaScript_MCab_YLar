<div class="col-md-12">
    <div class="row">
        <h2>Agregar Profesor</h2>
    </div>
    <br>
    <div class="row">
        <form class="form-horizontal" action="Profesor/crearProfesor" method="post">
            <div class="form-group">
                <label for="inputEmail3" class="col-sm-2 control-label">Nombres</label>
                <div class="col-sm-10">
                    <input type="text" name="nombre_Prof" class="form-control" id="inputEmail3" placeholder="Nombre de la materia">
                </div>
            </div>
            <div class="form-group">
                <label for="inputEmail3" class="col-sm-2 control-label">Apellidos</label>
                <div class="col-sm-10">
                    <input type="text" name="apellido_Prof" class="form-control" id="inputEmail3" placeholder="Nombre de la materia">
                </div>
            </div>
            <div class="form-group">
                <label for="inputEmail3" class="col-sm-2 control-label">E-mail</label>
                <div class="col-sm-10">
                    <input type="email" name="correo_Prof" class="form-control" id="inputEmail3" placeholder="Codigo de la materia">
                </div>
            </div>
            <br>
            <div class="form-group">
                <div class="col-md-offset-4 col-md-4">
                    <div class="btn-group btn-group-justified" role="group" aria-label="...">
                        <div class="btn-group" role="group">
                            <button type="submit" class="btn btn-primary">Guardar</button>
                        </div>
                        <div class="btn-group" role="group">
                            <a href="/formulario"><button type="button" class="btn btn-default">Cancelar</button></a>
                        </div>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>