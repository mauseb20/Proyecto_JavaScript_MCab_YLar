<div class="col-md-12">
    <div class="row">
        <h2>Agregar Laboratorio</h2>
    </div>
    <br>
    <div class="row">
        <form class="form-horizontal" action="/Laboratorio/editarLaboratorio" method="POST">
            <div class="form-group">
                <input type="text" style="display:none" value="<%= laboratorio.idLaboratorio%>" name="idLaboratorio">
                <label for="inputEmail3" class="col-sm-2 control-label">Nombre</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" name="nombreLaboratorio" value="<%= laboratorio.nombreLaboratorio%>">
                </div>
            </div>
            <div class="form-group form-inline">
                <label for="inputPassword3" class="col-sm-2 control-label">Aula</label>
                <div class="col-sm-4">
                    <input type="number" class="form-control" name="numAula" value="<%= laboratorio.numAula%>">
                </div>
                <label for="inputPassword3" class="col-sm-2 control-label">Capacidad</label>
                <div class="col-sm-3">
                    <input type="number" class="form-control" name="capacidad" value="<%= laboratorio.capacidad%>">
                </div>
            </div>
            <div class="form-group form-inline">
                <label for="inputPassword3" class="col-sm-2 control-label">Ordenadores</label>
                <div class="col-sm-4">
                    <input type="number" class="form-control" name="numOrdenadores" value="<%= laboratorio.numOrdenadores%>">
                </div>
                <label for="inputPassword3" class="col-sm-2 control-label">Proyector</label>
                <div class="col-sm-3">
                    <div class="checkbox">
                        <label>
                            <input type="checkbox" name="proyectorEmpotrado" value="SI" <%= (laboratorio.proyectorEmpotrado == 'SI') ? 'checked' :''%>> Empotrado
                        </label>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <label for="inputPassword3" class="col-sm-2 control-label">Ubicaci√≥n</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" name="descripcionUbicacion" value="<%= laboratorio.descripcionUbicacion%>">
                </div>
            </div>
            <br>
            <div class="form-group">
                <div class="col-md-offset-4 col-md-4">
                    <div class="btn-group btn-group-justified" role="group" aria-label="...">
                        <div class="btn-group" role="group">
                            <button type="submit" class="btn btn-primary">Guardar</button>
                        </div>
                        <div class="btn-group" role="group">
                            <a href="/laboratorios"><button type="button" class="btn btn-default">Cancelar</button></a>
                        </div>

                    </div>
                </div>
            </div>
        </form>
    </div>
</div>