<div class="col-md-12">
    <div class="row">
        <h2>Editar Laboratorio</h2>
    </div>
    <br>
    <div class="row">
        <form class="form-horizontal" action="" method="">
            <div class="form-group">
                <label for="inputEmail3" class="col-sm-2 control-label">Nombre</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="inputEmail3" placeholder="Nombre del Laboratorio">
                </div>
            </div>
            <div class="form-group form-inline">
                <label for="inputPassword3" class="col-sm-2 control-label">Aula</label>
                <div class="col-sm-4">
                    <input type="text" class="form-control" placeholder="">
                </div>
                <label for="inputPassword3" class="col-sm-2 control-label">Capacidad</label>
                <div class="col-sm-3">
                    <input type="text" class="form-control" id="inputEmail3" placeholder="">
                </div>
            </div>
            <div class="form-group form-inline">
                <label for="inputPassword3" class="col-sm-2 control-label">Ordenadores</label>
                <div class="col-sm-4">
                    <input type="text" class="form-control" placeholder="">
                </div>
                <label for="inputPassword3" class="col-sm-2 control-label">Proyector</label>
                <div class="col-sm-3">
                    <div class="checkbox">
                        <label>
                            <input type="checkbox"> Empotrado
                        </label>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <label for="inputPassword3" class="col-sm-2 control-label">Ubicación</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="inputEmail3" placeholder="Descripcion de la Ubicación">
                </div>
            </div>
            <br>
            <div class="form-group">
                <div class="col-md-offset-4 col-md-4">
                    <div class="btn-group btn-group-justified" role="group" aria-label="...">
                        <div class="btn-group" role="group">
                            <button type="submit" class="btn btn-primary">Guardar</button>
                        </div>
                        <div class="btn-group" role="group">
                            <a href="/laboratorios"><button type="button" class="btn btn-default">Cancelar</button></a>
                        </div>

                    </div>
                </div>
            </div>
        </form>
    </div>
</div>